{
  "name": "dnd-game",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently -k -p \"[{name}]\" -n \"Vite,Server\" -c \"yellow.bold,cyan.bold\" \"npm run dev:browser\" \"npm run dev:server\"",
    "dev:browser": "vite",
    "dev:server": "tsx watch --env-file=.env --tsconfig tsconfig.server.json server/index.ts",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@vueuse/core": "^11.0.3",
    "body-parser": "^1.20.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "lucide-vue-next": "^0.441.0",
    "mongoose": "^8.6.2",
    "radix-vue": "^1.9.5",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "vue": "^3.4.37",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@iconify-json/radix-icons": "^1.2.0",
    "@iconify/vue": "^4.1.2",
    "@types/body-parser": "^1.19.5",
    "@types/express": "^4.17.21",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^22.5.5",
    "@vitejs/plugin-vue": "^5.1.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "tailwindcss": "^3.4.11",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "vite": "^5.4.1",
    "vue-tsc": "^2.0.29"
  }
}
